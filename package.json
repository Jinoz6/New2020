    "scripts": {
        "dev": "concurrently \"yarn dev-build:watch\" \"yarn dev-js\" \"yarn watch-mix\" ",
        "dev-build:watch": "webpack --watch",
        "dev-js": "SET TZ='UTC' APP_NAME=saolao_pos_api NODE_ENV=development DEBUG=saolao_pos_api:* && nodemon ./dist/app.js",
        "dev-mix": "node node_modules/cross-env/src/bin/cross-env.js NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch-mix": "node node_modules/cross-env/src/bin/cross-env.js NODE_ENV=development node_modules/webpack/bin/webpack.js --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "production-mix": "node node_modules/cross-env/src/bin/cross-env.js NODE_ENV=production node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "js-build": "NODE_ENV=production webpack",
        "genkey": "node genKey.js",
        "test-build-run": "NODE_ENV=production node ./dist/app.js",
        "build": "concurrently \"yarn production-mix\" \"yarn js-build\" "
    }
}
